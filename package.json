{
  "name": "izimodal-wrap",
  "version": "1.0.0",
  "description": "A simple wrapper for iziModal",
  "main": "dist/commonjs/iziModalWrap.js",
  "typings": "dist/commonjs/iziModalWrap",
  "sass": "styles/iziModalWrap.scss",
  "scripts": {
    "tests": "ts-mocha --exit -r jsdom-global/register -p ./tsconfig.json src/**/*.spec.ts",
    "build": "npm run tests && rm -fr dist/* && node_modules/.bin/tsc"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/voltsonic/javascript-izimodal-wrap.git"
  },
  "author": "voltsonic",
  "license": "MIT",
  "dependencies": {
    "izimodal-1.6.0": "^1.6.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-fs": "^2.0.2",
    "@types/jquery": "^3.5.5",
    "@types/jsdom": "16.2.6",
    "@types/jsdom-global": "3.0.2",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "badge-management": "^1.0.2",
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "jsdom": "16.4.0",
    "jsdom-global": "3.0.2",
    "mocha": "^8.2.1",
    "ts-mocha": "^8.0.0",
    "typescript": "^4.1.3",
    "version-management": "^1.0.7"
  },
  "badges": {
    "injectors": {
      "README.md": [
        {
          "definitions": [
            "repository",
            "version"
          ]
        }
      ]
    },
    "definitions": {
      "version": {
        "type": "package_version"
      },
      "repository": {
        "type": "package_repository_url"
      }
    }
  }
}
